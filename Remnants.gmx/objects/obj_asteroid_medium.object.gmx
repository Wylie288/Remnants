    phy_speed_x = choose(0,0,random_range(0.1,1.1),random_range(1.1,2.1))
if x &gt; 480
    phy_speed_x = choose(0,0,random_range(-1.1,0.1),random_range(-2.1,-1.1))
if global.area = 1
    phy_speed_x /= (17/avs) 
if global.area = 2
    phy_speed_x /= (18/avs) 
if global.area = 3
    phy_speed_x /= (19/avs) 
}
if global.area = 4
phy_fixed_rotation = 0
phy_rotation = irandom_range(1,360)//Initial Roation
physics_apply_torque(irandom_range(-500,500))//Apply Rotation\=
color = make_color_hsv(irandom_range(0,55),irandom_range(50,125),irandom_range(255,230))
if global.area = 7
global.special_asteroids ++
global.mcount ++
</string>
